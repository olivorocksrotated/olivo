{
  "name": "olivo",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "debug": "NODE_OPTIONS='--inspect' next dev",
    "build": "next build",
    "start": "next start",
    "test": "npm run test:unit && npm run test:integration",
    "test:unit": "vitest run --coverage",
    "test:integration": "cypress run --component --quiet",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "vercel-build": "prisma generate --no-engine && next build",
    "db:login": "pscale auth login",
    "db:connect": "CURRENT_BRANCH=$(eval 'git branch --show-current | cut -f2- -d/'); pscale connect olivo ${npm_config_branch:-$CURRENT_BRANCH} --port 3310",
    "db:ls": "pscale branch list olivo",
    "db:gui": "prisma studio",
    "db:start-change": "CURRENT_BRANCH=$(eval 'git branch --show-current | cut -f2- -d/'); pscale branch create olivo ${npm_config_branch:-$CURRENT_BRANCH}",
    "db:apply-change": "prisma generate --no-engine",
    "db:push-change": "prisma db push",
    "db:finish-change": "CURRENT_BRANCH=$(eval 'git branch --show-current | cut -f2- -d/'); pscale branch delete olivo ${npm_config_branch:-$CURRENT_BRANCH}",
    "events:dev": "npx inngest-cli@latest dev -u http://localhost:3000/api/inngest",
    "vitest": "vitest",
    "storybook:dev": "storybook dev --ci -p 6006",
    "storybook:build": "storybook build",
    "cypress:open": "cypress open",
    "cypress:component": "cypress run --component",
    "danger:ci": "danger ci",
    "dependencies:check": "ncu",
    "dependencies:update": "ncu -u && npm install && npm test && npm run lint:fix",
    "analyze": "cross-env ANALYZE=true next build",
    "prepare": "husky install"
  },
  "dependencies": {
    "@auth/core": "^0.19.0",
    "@auth/prisma-adapter": "^1.0.12",
    "@formkit/auto-animate": "^1.0.0-beta.6",
    "@prisma/client": "^5.7.1",
    "@prisma/extension-accelerate": "^0.6.2",
    "@radix-ui/react-avatar": "^1.0.4",
    "@radix-ui/react-dialog": "^1.0.5",
    "@radix-ui/react-popover": "^1.0.7",
    "@radix-ui/react-select": "^2.0.0",
    "@radix-ui/react-tabs": "^1.0.4",
    "@radix-ui/react-tooltip": "^1.0.7",
    "@tiptap/extension-bubble-menu": "^2.1.13",
    "@tiptap/extension-mention": "^2.1.13",
    "@tiptap/extension-task-item": "^2.1.13",
    "@tiptap/extension-task-list": "^2.1.13",
    "@tiptap/extension-typography": "^2.1.13",
    "@tiptap/html": "^2.1.13",
    "@tiptap/pm": "^2.1.13",
    "@tiptap/react": "^2.1.13",
    "@tiptap/starter-kit": "^2.1.13",
    "@tiptap/suggestion": "^2.1.13",
    "@tremor/react": "^3.12.1",
    "@vercel/analytics": "^1.1.1",
    "ai": "^2.2.30",
    "clsx": "^2.0.0",
    "cmdk": "^0.2.0",
    "cva": "npm:class-variance-authority@^0.7.0",
    "date-fns": "^3.0.6",
    "framer-motion": "^10.16.16",
    "inngest": "^3.7.4",
    "next": "14.0.4",
    "next-auth": "^5.0.0-beta.4",
    "next-safe-action": "^5.2.3",
    "openai-edge": "^1.2.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.12.0",
    "react-markdown": "^9.0.1",
    "react-toastify": "^9.1.3",
    "remark-gfm": "^4.0.0",
    "tippy.js": "^6.3.7",
    "zod": "^3.22.4"
  },
  "devDependencies": {
    "@cypress/webpack-preprocessor": "^6.0.1",
    "@next/bundle-analyzer": "^14.0.4",
    "@storybook/addon-essentials": "^7.6.6",
    "@storybook/addon-interactions": "^7.6.6",
    "@storybook/addon-links": "^7.6.6",
    "@storybook/blocks": "^7.6.6",
    "@storybook/nextjs": "^7.6.6",
    "@storybook/react": "^7.6.6",
    "@storybook/testing-library": "^0.2.2",
    "@testing-library/react": "^14.1.2",
    "@types/node": "^20.10.5",
    "@types/react": "^18.2.46",
    "@types/react-dom": "^18.2.18",
    "@typescript-eslint/eslint-plugin": "^6.16.0",
    "@vitejs/plugin-react": "^4.2.1",
    "@vitest/coverage-v8": "^1.1.0",
    "autoprefixer": "^10.4.16",
    "cross-env": "^7.0.3",
    "cypress": "^13.6.2",
    "danger": "^11.3.1",
    "eslint": "^8.56.0",
    "eslint-config-next": "^14.0.4",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-storybook": "^0.6.15",
    "eslint-plugin-tailwindcss": "^3.13.1",
    "husky": "^8.0.3",
    "jsdom": "^23.0.1",
    "lint-staged": "^15.2.0",
    "npm-check-updates": "^16.14.12",
    "postcss": "^8.4.32",
    "prisma": "^5.7.1",
    "prismock": "^1.24.0",
    "storybook": "^7.6.6",
    "tailwindcss": "^3.4.0",
    "typescript": "^5.3.3",
    "vitest": "^1.1.0",
    "vitest-mock-extended": "^1.3.1"
  }
}
