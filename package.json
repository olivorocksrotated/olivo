{
  "name": "olivo",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "debug": "NODE_OPTIONS='--inspect' next dev",
    "build": "next build",
    "start": "next start",
    "test": "npm run test:unit && npm run test:integration",
    "test:unit": "vitest run --coverage",
    "test:integration": "cypress run --component --quiet",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "vercel-build": "prisma generate && next build",
    "db:login": "pscale auth login",
    "db:connect": "CURRENT_BRANCH=$(eval 'git branch --show-current | cut -f2- -d/'); pscale connect olivo ${npm_config_branch:-$CURRENT_BRANCH} --port 3310",
    "db:ls": "pscale branch list olivo",
    "db:gui": "prisma studio",
    "db:start-change": "CURRENT_BRANCH=$(eval 'git branch --show-current | cut -f2- -d/'); pscale branch create olivo ${npm_config_branch:-$CURRENT_BRANCH}",
    "db:apply-change": "prisma generate",
    "db:push-change": "prisma db push",
    "db:finish-change": "CURRENT_BRANCH=$(eval 'git branch --show-current | cut -f2- -d/'); pscale branch delete olivo ${npm_config_branch:-$CURRENT_BRANCH}",
    "events:dev": "npx inngest-cli@latest dev -u http://localhost:3000/api/inngest",
    "vitest": "vitest",
    "storybook:dev": "storybook dev --ci -p 6006",
    "storybook:build": "storybook build",
    "cypress:open": "cypress open",
    "cypress:component": "cypress run --component",
    "danger:ci": "danger ci",
    "dependencies:check": "ncu",
    "dependencies:update": "ncu -u && npm install && npm test && npm run lint:fix",
    "analyze": "cross-env ANALYZE=true next build",
    "prepare": "husky install"
  },
  "dependencies": {
    "@formkit/auto-animate": "^1.0.0-beta.6",
    "@next-auth/prisma-adapter": "^1.0.7",
    "@prisma/client": "^5.2.0",
    "@radix-ui/react-avatar": "^1.0.3",
    "@radix-ui/react-dialog": "^1.0.4",
    "@radix-ui/react-popover": "^1.0.6",
    "@radix-ui/react-select": "^1.2.2",
    "@radix-ui/react-tabs": "^1.0.4",
    "@radix-ui/react-tooltip": "^1.0.6",
    "@tiptap/extension-typography": "^2.1.8",
    "@tiptap/html": "^2.1.8",
    "@tiptap/pm": "^2.1.8",
    "@tiptap/react": "^2.1.8",
    "@tiptap/starter-kit": "^2.1.8",
    "@tremor/react": "^3.7.4",
    "@vercel/analytics": "^1.0.2",
    "ai": "^2.2.12",
    "clsx": "^2.0.0",
    "cmdk": "^0.2.0",
    "cva": "npm:class-variance-authority@^0.7.0",
    "date-fns": "^2.30.0",
    "framer-motion": "^10.16.4",
    "inngest": "^2.6.0",
    "next": "13.4.19",
    "next-auth": "^4.23.1",
    "openai-edge": "^1.2.2",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.11.0",
    "react-markdown": "^8.0.7",
    "react-toastify": "^9.1.3",
    "remark-gfm": "^3.0.1",
    "zact": "^0.0.2",
    "zod": "^3.22.2"
  },
  "devDependencies": {
    "@cypress/webpack-preprocessor": "^6.0.0",
    "@next/bundle-analyzer": "^13.4.19",
    "@storybook/addon-essentials": "^7.4.0",
    "@storybook/addon-interactions": "^7.4.0",
    "@storybook/addon-links": "^7.4.0",
    "@storybook/blocks": "^7.4.0",
    "@storybook/nextjs": "^7.4.0",
    "@storybook/react": "^7.4.0",
    "@storybook/testing-library": "^0.2.0",
    "@testing-library/react": "^14.0.0",
    "@types/node": "^20.6.0",
    "@types/react": "^18.2.21",
    "@types/react-dom": "^18.2.7",
    "@typescript-eslint/eslint-plugin": "^6.6.0",
    "@vitejs/plugin-react": "^4.0.4",
    "@vitest/coverage-c8": "^0.33.0",
    "@vitest/coverage-v8": "^0.34.4",
    "autoprefixer": "^10.4.15",
    "cross-env": "^7.0.3",
    "cypress": "^13.1.0",
    "danger": "^11.2.8",
    "eslint": "^8.49.0",
    "eslint-config-next": "^13.4.19",
    "eslint-plugin-react": "^7.33.2",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-storybook": "^0.6.13",
    "eslint-plugin-tailwindcss": "^3.13.0",
    "husky": "^8.0.3",
    "jsdom": "^22.1.0",
    "lint-staged": "^14.0.1",
    "npm-check-updates": "^16.13.3",
    "postcss": "^8.4.29",
    "prisma": "^5.2.0",
    "prismock": "^1.21.0",
    "storybook": "^7.4.0",
    "tailwindcss": "^3.3.3",
    "typescript": "^5.2.2",
    "vitest": "^0.34.4",
    "vitest-mock-extended": "^1.2.1"
  }
}
