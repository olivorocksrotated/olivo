{
  "name": "olivo",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "next build",
    "start": "next start",
    "test": "npm run test:unit && npm run test:integration",
    "test:unit": "vitest run --coverage",
    "test:integration": "cypress run --component --quiet",
    "lint": "next lint",
    "lint:fix": "next lint --fix",
    "vercel-build": "prisma generate && next build",
    "db:login": "pscale auth login",
    "db:connect": "CURRENT_BRANCH=$(eval 'git branch --show-current | cut -f2- -d/'); pscale connect olivo ${npm_config_branch:-$CURRENT_BRANCH} --port 3310",
    "db:ls": "pscale branch list olivo",
    "db:gui": "prisma studio",
    "db:start-change": "CURRENT_BRANCH=$(eval 'git branch --show-current | cut -f2- -d/'); pscale branch create olivo ${npm_config_branch:-$CURRENT_BRANCH}",
    "db:apply-change": "prisma generate",
    "db:push-change": "prisma db push",
    "db:finish-change": "CURRENT_BRANCH=$(eval 'git branch --show-current | cut -f2- -d/'); pscale branch delete olivo ${npm_config_branch:-$CURRENT_BRANCH}",
    "vitest": "vitest",
    "cypress:open": "cypress open",
    "cypress:component": "cypress run --component",
    "danger:ci": "danger ci",
    "dependencies:check": "ncu",
    "dependencies:update": "ncu -u && npm install && npm test && npm run lint:fix",
    "prepare": "husky install"
  },
  "dependencies": {
    "@formkit/auto-animate": "^1.0.0-beta.6",
    "@next-auth/prisma-adapter": "^1.0.6",
    "@prisma/client": "^4.14.0",
    "@radix-ui/react-avatar": "^1.0.2",
    "@radix-ui/react-dialog": "^1.0.3",
    "@radix-ui/react-popover": "^1.0.5",
    "@types/node": "^20.1.4",
    "@types/react": "^18.2.6",
    "@types/react-dom": "^18.2.4",
    "@vercel/analytics": "^1.0.1",
    "clsx": "^1.2.1",
    "date-fns": "^2.30.0",
    "eslint": "^8.40.0",
    "eslint-config-next": "^13.4.2",
    "framer-motion": "^10.12.10",
    "next": "^13.4.2",
    "next-auth": "^4.22.1",
    "react": "^18.2.0",
    "react-dom": "^18.2.0",
    "react-icons": "^4.8.0",
    "typescript": "^5.0.4",
    "zact": "^0.0.2",
    "zod": "^3.21.4"
  },
  "devDependencies": {
    "@cypress/webpack-preprocessor": "^5.17.1",
    "@testing-library/react": "^14.0.0",
    "@typescript-eslint/eslint-plugin": "^5.59.5",
    "@vitejs/plugin-react": "^4.0.0",
    "@vitest/coverage-c8": "^0.31.0",
    "autoprefixer": "^10.4.14",
    "cypress": "^12.12.0",
    "danger": "^11.2.6",
    "eslint-plugin-react": "^7.32.2",
    "eslint-plugin-simple-import-sort": "^10.0.0",
    "eslint-plugin-tailwindcss": "^3.12.0",
    "husky": "^8.0.3",
    "jsdom": "^22.0.0",
    "lint-staged": "^13.2.2",
    "npm-check-updates": "^16.10.12",
    "postcss": "^8.4.23",
    "prisma": "^4.14.0",
    "prismock": "^1.13.2",
    "tailwindcss": "^3.3.2",
    "vitest": "^0.31.0",
    "vitest-mock-extended": "^1.1.3"
  }
}
